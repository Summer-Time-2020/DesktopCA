#ifndef HELP_H
#define HELP_H

#define HELP_INITIALIZA                                               \
{                                                                       \
    qmlRegisterType<Welcome::Manage>("Help", 1, 0, "HelpManage"); \
}

namespace Help
{

class Manage
{
    Q_OBJECT
    Q_DISABLE_COPY(Manage)

public:
    Manage() = default;

    ~Manage() = default;

public slots:
    void openGitHubLink();

    void openLatestVersionDownloadLink();
};

}

#endif//HELP_H
